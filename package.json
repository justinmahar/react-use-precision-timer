{
  "name": "react-use-precision-timer",
  "version": "1.0.5",
  "author": "DevBoldly <devboldly@gmail.com>",
  "description": "A versatile precision timer hook for React. Doubles as a stopwatch.",
  "homepage": "https://devboldly.github.io/react-use-precision-timer/",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "npm run docz:build",
    "deploy": "gh-pages -d .docz/dist",
    "preship": "npm run build && git add dist && git diff-index --quiet HEAD || git commit -m \"Build, version, and publish.\" && npm version patch -m \"Build, version, and publish.\"",
    "ship": "npm publish --access public",
    "postship": "git push",
    "docz": "npm run docz:clean && npm run docz:dev",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "docz:clean": "rm -rf .docz/.cache"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devboldly/react-use-precision-timer.git"
  },
  "bugs": {
    "url": "https://github.com/devboldly/react-use-precision-timer/issues"
  },
  "keywords": [],
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "babel-loader": "^8.0.6",
    "dboldly-react-tools": "^1.0.16",
    "docz": "^2.3.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-app": "^6.2.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^6.0.0",
    "react-test-renderer": "^16.13.0",
    "react-use-window-scroll": "^1.0.9",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  }
}
